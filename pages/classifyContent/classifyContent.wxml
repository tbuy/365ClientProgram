<view class='container-p'>

  <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{imgUrls}}" wx:key="{{item.id}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image" />
      </swiper-item>
    </block>
  </swiper>

  <view class="list theme_wordSize_small theme_word_deep">
    <view wx:for="{{8}}" wx:key="{{item.id}}" class="list-item">
      <view class="item">
        <image class="item-image public_avatar" src="{{icon||'/images/avatar.png'}}"></image>
        <view class="intro">
          <view>name</view>
          <view>
            <text>47</text>
            <text>辽宁</text>
            <text>从业4年</text>
          </view>
        </view>
        <view class="theme_btn btn">咨询价格</view>
      </view>
      <view class="remark theme_word_normal">备注：eeeee</view>
    </view>
  </view>

</view>